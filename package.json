    "next": "10.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@namics/eslint-config": "9.0.2",
    "@namics/prettier-config": "1.1.0",
    "@namics/ts-config": "1.0.1",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-links": "6.0.28",
